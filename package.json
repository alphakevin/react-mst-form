{
    "name": "react-mst-form",
    "version": "1.0.3",
    "description": "react-mobx-state-tree-form",
    "main": "./dist/react-mst-form.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/naguvan/react-mst-form.git",
    "author": "naguvan <naguvan@outlook.com>",
    "license": "MIT",
    "private": false,
    "bugs": {
        "url": "https://github.com/naguvan/react-mst-form/issues"
    },
    "files": [
        "dist/"
    ],
    "peerDependencies": {
        "classnames": "^2.2.5",
        "jss": "^9.8.1",
        "jss-preset-default": "^4.3.0",
        "mobx": "5.0.3",
        "mobx-react": "5.2.3",
        "mobx-state-tree": "2.2.0",
        "react": "^16.3.0",
        "react-dom": "^16.3.0"
    },
    "dependencies": {
        "@material-ui/core": "^1.3.1",
        "@material-ui/icons": "^1.1.0",
        "classnames": "^2.2.5",
        "jss": "^9.8.1",
        "jss-preset-default": "^4.3.0",
        "mobx": "^4.1.1",
        "mobx-react": "^5.0.0",
        "mobx-state-tree": "^2.0.2",
        "react": "^16.3.0",
        "react-dom": "^16.3.0",
        "tslib": "^1.9.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.3",
        "@types/express": "^4.11.1",
        "@types/jest": "^22.2.2",
        "@types/node": "^9.6.1",
        "@types/react": "^16.1.0",
        "@types/react-dom": "^16.0.4",
        "@types/webpack": "^4.1.3",
        "cross-env": "^5.1.4",
        "jest": "^22.4.3",
        "ts-jest": "^22.4.2",
        "ts-loader": "^4.1.0",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-loader": "^3.6.0",
        "tslint-react": "^3.5.1",
        "typescript": "2.9.2",
        "webpack": "^4.4.1",
        "webpack-bundle-analyzer": "^2.11.1",
        "webpack-cli": "^2.0.13"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "modulePaths": [
            "<rootDir>/src/"
        ],
        "roots": [
            "<rootDir>/src/"
        ]
    },
    "scripts": {
        "prepare": "npm run lint && npm run test && npm run prod",
        "lint": "tslint -p .",
        "test": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"declaration\\\":false} jest --runInBand",
        "env": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"}",
        "stats": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod --profile --json > stats.json",
        "prod": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod",
        "dev": "cross-env NODE_ENV=development TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts",
        "demo": "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./demo/server.ts",
        "analyzer": "webpack-bundle-analyzer ./stats.json",
        "tsc": "tsc",
        "build": "npm run tsc"
    }
}