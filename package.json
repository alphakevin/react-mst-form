{
  "name": "react-mst-form",
  "version": "1.0.9",
  "description": "react-mobx-state-tree-form",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": "https://github.com/naguvan/react-mst-form.git",
  "author": "naguvan <naguvan@outlook.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/naguvan/react-mst-form/issues"
  },
  "files": [
    "lib/",
    "lib-esm/"
  ],
  "peerDependencies": {
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "mobx-state-tree": "^2.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "@material-ui/core": "3.0.1",
    "@material-ui/icons": "3.0.1",
    "classnames": "2.2.6",
    "jss": "9.8.7",
    "jss-preset-default": "4.5.0",
    "mobx": "5.1.0",
    "mobx-react": "5.2.5",
    "mobx-state-tree": "2.2.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/node": "10.9.3",
    "@types/react": "16.4.12",
    "@types/react-dom": "16.0.7",
    "@types/webpack": "4.4.11",
    "cross-env": "5.2.0",
    "jest": "23.5.0",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "husky": "0.14.3",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "typescript": "3.0.1",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "scripts": {
    "prepare": "npm run lint && npm run test && npm run build",
    "lint": "tslint -p .",
    "precommit": "npm run lint && pretty-quick --staged",
    "pretty": "prettier --write '{src,demo}/**/*.{ts,tsx}'",
    "test": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"declaration\\\":false} jest --runInBand",
    "env": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"}",
    "stats": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod --profile --json > stats.json",
    "prod": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod",
    "dev": "cross-env NODE_ENV=development TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts",
    "demo": "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./demo/server.ts",
    "analyzer": "webpack-bundle-analyzer ./stats.json",
    "dev:tsc": "tsc && tsc -m es6 --outDir lib-esm",
    "build": "npm run dev:tsc"
  }
}
