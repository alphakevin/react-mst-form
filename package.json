{
    "name": "react-mst-form",
    "version": "1.0.0",
    "description": "react-mobx-state-tree-form",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/naguvan/react-mst-form.git",
    "author": "naguvan <naguvan@outlook.com>",
    "license": "MIT",
    "private": false,
    "files": [
        "dist"
    ],
    "dependencies": {
        "classnames": "^2.2.5",
        "jss": "^9.8.0",
        "jss-preset-default": "^4.3.0",
        "material-ui": "^1.0.0-beta.34",
        "material-ui-icons": "^1.0.0-beta.17",
        "mobx": "^3.5.1",
        "mobx-react": "^4.4.2",
        "mobx-state-tree": "^1.3.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.3",
        "@types/express": "^4.11.1",
        "@types/jest": "^22.1.3",
        "@types/node": "^9.4.6",
        "@types/react": "^16.0.38",
        "@types/react-dom": "^16.0.4",
        "@types/webpack": "^3.8.8",
        "cross-env": "^5.1.3",
        "jest": "^22.3.0",
        "ts-jest": "^22.0.4",
        "ts-loader": "^3.5.0",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.8.0",
        "tslint-loader": "^3.5.3",
        "tslint-react": "^3.5.1",
        "typescript": "^2.7.2",
        "webpack": "^3.11.0",
        "webpack-bundle-analyzer": "^2.10.0"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
        "modulePaths": ["<rootDir>/src/"]
    },
    "scripts": {
        "prepublish": "yarn run lint && yarn run test && yarn run bundle",
        "build": "tsc -p .",
        "lint": "tslint -p .",
        "test": "jest",
        "env": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"}",
        "stats":
            "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod --profile --json > stats.json",
        "bundle":
            "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts --env.prod",
        "dev":
            "cross-env NODE_ENV=development TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} webpack --config webpack.config.ts",
        "demo":
            "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./demo/server.ts",
        "analyzer": "webpack-bundle-analyzer ./stats.json"
    }
}
