{
  "name": "reactive-json-schema",
  "version": "1.0.0",
  "description": "reactive json schema validator using mobx-state-tree",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "skclusive <skclusive@outlook.com>",
  "license": "ISC",
  "files": [
    "lib/",
    "lib-esm/"
  ],
  "keywords": [
    "reactive",
    "mobx-state-tree",
    "mst",
    "mobx",
    "json",
    "schema",
    "typescript"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "prebuild": "rimraf {lib,lib-esm}",
    "lint": "tslint -p .",
    "precommit": "npm run lint && pretty-quick --staged",
    "pretty": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\",\\\"declaration\\\":false} jest --runInBand",
    "dev:tsc": "tsc && tsc -m es6 --outDir lib-esm",
    "build": "npm run dev:tsc"
  },
  "dependencies": {
    "mobx": "5.5.0",
    "mobx-state-tree": "2.2.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.3",
    "cross-env": "5.2.0",
    "husky": "0.14.3",
    "jest": "23.6.0",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.1",
    "ts-loader": "5.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.0.3"
  }
}
